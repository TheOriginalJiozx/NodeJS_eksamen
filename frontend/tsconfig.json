{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": ["svelte"],
    "rootDir": ".",
    "outDir": ".ts-out"
  },
  "include": ["src", ".svelte-kit"],
  "exclude": ["node_modules"]
}
